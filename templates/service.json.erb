<% require "json" -%>
{
  "port": <%= @port -%>,
  "host": "<%= @host -%>",
  "zk_hosts": <%= @zk_hosts -%>,
  "zk_path": "<%= @zk_path -%>",
  "check_interval": <%= @check_interval -%>,
<% if !@labels.nil? && !@labels.empty? -%>
  "labels": <%= JSON.generate(@labels) %>,
<% end -%>
  "checks": <%= JSON.generate(@checks) %>
}
